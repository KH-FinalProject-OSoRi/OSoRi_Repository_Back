package com.kh.osori.group.dao;

import java.util.List;
import org.mybatis.spring.SqlSessionTemplate;
import org.springframework.stereotype.Repository;
import com.kh.osori.group.model.vo.GroupBudget;

@Repository
public class GroupBudgetDao {

    public List<GroupBudget> selectGroupList(SqlSessionTemplate sqlSession, int userId) {
        // mapper의 namespace.id를 호출합니다.
        return sqlSession.selectList("groupMapper.selectGroupList", userId);
    }
}