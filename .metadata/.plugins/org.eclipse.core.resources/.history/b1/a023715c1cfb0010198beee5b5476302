package com.kh.osori.group.service;

import java.util.List;
import org.mybatis.spring.SqlSessionTemplate;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;
import com.kh.osori.group.dao.GroupBudgetDao;
import com.kh.osori.group.model.vo.GroupBudget;

@Service
public class GroupBudgetServiceImpl implements GroupBudgetService {
    
    @Autowired
    private GroupBudgetDao dao;
    
    @Autowired
    private SqlSessionTemplate sqlSession;

    @Override
    public List<GroupBudget> selectGroupList(int userId) {
        return dao.selectGroupList(sqlSession, userId);
    }
}