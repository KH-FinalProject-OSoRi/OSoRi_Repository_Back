<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace= "faqMapper">
	<select id="questionList" resultType="Faq">
		SELECT FAQ_ID, QUESTION, ANSWER
		FROM FAQ
	</select>
	
	<insert id="addNewQuestion" parameterType="String">
		INSERT INTO FAQ VALUES (FAQ_NO.NEXTVAL, #{question}, '답변 필요')
	</insert>

</mapper>